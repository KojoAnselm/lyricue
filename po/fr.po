#: ../lyricue:261
msgid ""
"\n"
"Usage: lyricue <-v|-l> <-b> <-k> <-d[q]> <-s>\n"
"\n"
msgstr ""
"\n"
"Syntaxe : lyricue <-v|-l> <-b> <-k> <-d[q]> <-s>\n"
"\n"

#: ../lyricue:262
msgid "\t-v:  Prints Lyricue version information & exits\n"
msgstr "\t-v :  Affiche l'information de version de Lyricue et quitte\n"

#: ../lyricue:263
msgid "\t-l:  Outputs song list in HTML & exits\n"
msgstr "\t-l :  Affiche la liste des chants en HTML et quitte\n"

#: ../lyricue:264
msgid "\t-b:  Loads Lyricue without background previews\n"
msgstr "\t-b :  Lance Lyricue sans les aperçus d'arrière-plan\n"

#: ../lyricue:265
msgid "\t-s:  Specify the host on which the lyric server is located\n"
msgstr "\t-s :  Indique l'emplacement du serveur de paroles\n"

#: ../lyricue:266
msgid "\t-d:  Prints debugging messages\n"
msgstr "\t-d :  Affiche les informations de débogage\n"

#: ../lyricue:267
msgid "\t-w:  Don't maximize on startup\n"
msgstr "\t-w :  Ne pas maximiser la fenêtre au démarrage\n"

#: ../lyricue:268
msgid ""
"\t-dq: Prints debugging messages + sql\n"
"\n"
msgstr ""
"\t-dq : Affiche les messages de débogage et les commandes SQL\n"
"\n"

